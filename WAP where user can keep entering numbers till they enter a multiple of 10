#include<iostream>
using namespace std;

int main(){
    cout<<endl;
    int n;

    while(true){
        cout<<"Enter a number: ";
        cin>>n;
        if(n%10==0){
            cout<<"Multiple of 10"<<endl;
            break;
        }

        else{
            cout<<"Not a multiple of 10"<<endl;
        }
    }
    return 0;
}
